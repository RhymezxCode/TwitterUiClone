<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/space_24" />
            <solid android:color="?abstractColorCellBackground" />
            <stroke android:width="1.0px" android:color="?abstractColorText" />
        </shape>
    </item>
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/space_24" />
            <solid android:color="?abstractColorText" />
            <stroke android:width="1.0px" android:color="?abstractColorText" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/space_24" />
            <solid android:color="?abstractColorCellBackground" />
            <stroke android:width="@dimen/space_1" android:color="?abstractColorLightGray" />
        </shape>
    </item>
</selector>