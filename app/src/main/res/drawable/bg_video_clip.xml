<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background" android:drawable="@drawable/ic_selectedclip" />
    <item android:drawable="@drawable/ic_selectedclip_pressed" />
    <item android:id="@android:id/progress">
        <clip android:gravity="left" android:clipOrientation="horizontal"
          xmlns:android="http://schemas.android.com/apk/res/android">
            <nine-patch android:src="@drawable/ic_selectedclip_playback" android:dither="true" />
        </clip>
    </item>
    <item android:drawable="@drawable/ic_selectedclip_gripper" />
    <item>
        <bitmap android:gravity="center" android:src="@drawable/ic_selectedclip_dots" />
    </item>
</layer-list>